{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory0523"
		},
		"BatchLink563_accessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accessKey' of 'BatchLink563'"
		},
		"projectStorageLink1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'projectStorageLink1'"
		},
		"BatchLink563_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://batchaccount1.westus2.batch.azure.com"
		},
		"BatchLink563_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "563-pool"
		},
		"BatchLink563_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "batchaccount1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory0523_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"projectStorageLink1_connectionString": {
						"value": "[parameters('projectStorageLink1_connectionString')]"
					},
					"BatchLink563_accessKey": {
						"value": "[parameters('BatchLink563_accessKey')]"
					},
					"BatchLink563_properties_typeProperties_batchUri": {
						"value": "[parameters('BatchLink563_properties_typeProperties_batchUri')]"
					},
					"BatchLink563_properties_typeProperties_poolName": {
						"value": "[parameters('BatchLink563_properties_typeProperties_poolName')]"
					},
					"BatchLink563_properties_typeProperties_accountName": {
						"value": "[parameters('BatchLink563_properties_typeProperties_accountName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}